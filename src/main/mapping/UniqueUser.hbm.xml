<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.adsapient.shared.mappable.UniqueUser"
		table="uniqueuser" lazy="false">
		<cache usage="read-write" />
		<id name="userId" type="int" unsaved-value="null">
			<column name="USER_ID" sql-type="int(11)" not-null="true" />
			<generator class="increment" />
		</id>

		<property name="userIp">
			<column name="USER_IP" sql-type="varchar(128)"
				not-null="true" />
		</property>

		
		
		

	</class>

</hibernate-mapping>






