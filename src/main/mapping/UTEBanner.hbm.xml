<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.adsapient.api_impl.ute.UTEBanner" table="utebanner" lazy="false" >
		<cache usage="read-write" />
       
        <id name="id" type="int" unsaved-value="null"  >
            <column name="ID" sql-type="int(11)" not-null="true" />
            <generator class="increment"/>
        </id>

        <property name="bannerId">
           <column name="BANNER_ID" sql-type="int(11)" not-null="true" />
        </property>

        <property name="location">
            <column name="LOCATION" sql-type="varchar(240)" not-null="true"/>
        </property>

        <property name="price">
        	<column name="PRICE" sql-type="int(10)" not-null="true" ></column>
       </property> 
        <property name="date">
            <column name="START_DATE" sql-type="varchar(10)" not-null="false"/>
        </property>                    
    </class>

</hibernate-mapping>