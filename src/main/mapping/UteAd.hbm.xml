<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.adsapient.api_impl.ute.UteAdImpl" table="utead" lazy="false">
		<cache usage="read-write"  />

		<id name="bannerId" type="int" unsaved-value="null">
			<column name="BANNER_ID" sql-type="int(11)" not-null="true"/>
			<generator class="hilo">
                <param name="table">banneridgenerator</param>
                <param name="column">ID</param>
                <param name="max_lo">10</param>
        	</generator>
		</id>

		<property name="userId">
			<column name="USER_ID" sql-type="int(11)" not-null="true"/>
		</property>

		<property name="campainId">
			<column name="CAMPAIN_ID" sql-type="int(11)" not-null="true"/>
		</property>

		<property name="sizeId">
			<column name="SIZE_ID" sql-type="int(11)" not-null="true"/>
		</property>
		
		<property name="typeId">
			<column name="TYPE_ID" sql-type="int(11)" not-null="true"/>
		</property>
		
		<property name="file">
			<column name="FILE_TEST"  not-null="true"/>
		</property>
		
		<property name="fileName">
			<column name="FILE_NAME" sql-type="varchar(254)" not-null="true"/>
		</property>
		
		<property name="stateId">
			<column name="BANNER_STATE_ID" sql-type="varchar(128)" not-null="true"/>
		</property>
		
		<property name="contentType">
			<column name="CONTENTTYPE" sql-type="varchar(64)"/>
		</property>

		<property name="url">
			<column name="URL" sql-type="text" not-null="true"/>
		</property>

		<property name="prioritet">
			<column name="BANNER_PRIOR" sql-type="int(6)" not-null="true"/>
		</property>

		<property name="status">
			<column name="STATUS" sql-type="int(2)"/>
		</property>

		<property name="rateId" type="int">
			<column name="RATE_ID" not-null="false"/>
		</property>

		<property name="altText">
			<column name="ALT_TEXT" sql-type="varchar(240)" not-null="true"/>
		</property>
		
		<property name="statusBartext">
			<column name="STATUSBAR_TEXT" sql-type="varchar(240)" not-null="true"/>
		</property>
		
		<property name="loadingTypeId">
			<column name="LOADINGTYPE_ID" sql-type="int(6)" not-null="true"/>
		</property>
		
		<property name="placeTypeId">
			<column name="PLACETYPE_ID" sql-type="int(6)" not-null="true"/>
		</property>
		
		<property name="targetWindowId">
			<column name="TARGETWINDOW_ID" sql-type="int(6)" not-null="true"/>
		</property>
		
		<property name="name">
			<column name="BANNER_NAME" sql-type="varchar(200)" not-null="true"/>
		</property>
		
		<property name="startDate">
			<column name="START_DATE" sql-type="varchar(20)" not-null="true"/>
		</property>
		<property name="resourceId">
			<column name="RESOURCE_ID" sql-type="int(6)" not-null="true"/>
		</property>
	</class>

</hibernate-mapping>

