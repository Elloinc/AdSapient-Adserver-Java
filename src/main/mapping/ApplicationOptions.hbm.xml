<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Ma DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
                            
    <class name="com.adsapient.api_impl.share.ApplicationOptImpl" table="application" lazy="false">
              <cache usage="read-write" />
       
        <id name="id" type="int" unsaved-value="null"  >
            <column name="ID" sql-type="int(11)" not-null="true" />
            <generator class="increment"/>
        </id>
       
        <property name="name">
             <column name="NAME" sql-type="varchar(200)" not-null="true"/>
        </property>   
     
        <property name="value">
             <column name="VALUE" sql-type="varchar(200)" not-null="true"/>
        </property> 
           
    </class>
</hibernate-mapping>

